{
  "name": "mroelectric/formidable",
  "type": "project",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "^2",
    "phpunit/phpunit": "^10",
    "squizlabs/php_codesniffer": "^3",
    "vimeo/psalm": "^5",
    "webimpress/coding-standard": "^1"
  },
  "autoload": {
    "psr-4": {
      "Formidable\\": "src/Formidable",
      "Test\\Unit\\": "test/unit"
    }
  },
  "scripts": {
    "cs": "phpcs",
    "cs-fix": "phpcbf",
    "psalm": "psalm",
    "test": "phpunit"
  }
}
